<% unless @inactive_camps.empty? %>
	<div class="i-cont"><h3>Inactive Camps</h3></div>
  <div class="panel clearfix">
    <ul class="side-nav">
      <% @inactive_camps.each do |camp| %>
        <li><%= link_to camp.name, camp_path(camp) %></li>
      <% end %>
    </ul>
  </div>
<% else %>
	<div class="i-cont"><h3>No Inactive Camps</h3></div>
<% end %>