<% unless @inactive_students.empty? %>
	<h3>Inactive Students</h3>
	<div class="panel clearfix">
		<ul class="side-nav">
			<% @inactive_students.each do |student| %>
				<li><%= link_to "#{student.name}", student_path(student) %></li>
			<% end %>
		</ul>
	</div>
<% else %>
	<h3>No Inactive Students</h3>
<% end %>
